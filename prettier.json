{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "ignore",
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 70,
  "proseWrap": "preserve" ,
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
